<div>
    <h1>Ironhack Cinema</h1>

    <div>
       <h1>Movie List</h1>
<ul>
    {{#each movies}}
    <li>
        <h2>{{this.title}}</h2>
        <img src="{{this.image}}" alt="{{this.title}} image">
        <a href="/movies/{{this._id}}">See more</a> 
    </li>
    {{/each}}
</ul>
    </div>
</div>